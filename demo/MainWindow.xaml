<Window x:Class="demo.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:demo"
        mc:Ignorable="d"
        Title="MainWindow" Height="600" Width="1200">
    <Grid x:Name="grid1" Margin="0,0,0,0">
        <TextBlock HorizontalAlignment="Left" Margin="10,10,0,0" TextWrapping="Wrap" Text="GROUP MANAGEMENT INFORMATION SYSTEM" VerticalAlignment="Top" Height="60" Width="771" FontSize="36"/>
        <TextBox x:Name="search" HorizontalAlignment="Left" Margin="824,10,0,0" TextWrapping="Wrap" Text="search    " VerticalAlignment="Top" Width="300" Height="60" FontSize="36"/>
        <ListBox x:Name="student_list" d:ItemsSource="{Binding Source={StaticResource groupList}}"  Margin="0,100,0,0"/>

    </Grid>
</Window>
